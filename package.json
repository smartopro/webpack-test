{
  "name": "webpack-test",
  "version": "1.0.0",
  "description": "webpack test",
  "private": true,
  "browserslist": "> 0.01%, not dead",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "devserver": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "nasa": "cross-env NODE_ENV=development webpack-dashboard -- npm run dev",
    "webpack-debug": "cross-env NODE_ENV=development node --inspect node_modules\\webpack\\bin\\webpack.js",
    "nodemon-debug": "cross-env NODE_ENV=development nodemon --inspect --watch webpack.config.js --watch src node_modules\\webpack\\bin\\webpack.js",
    "nodemon": "cross-env NODE_ENV=development nodemon --watch **/*.* node_modules\\webpack\\bin\\webpack.js",
    "stats": "webpack --json > dist/release/stats.json && webpack-bundle-analyzer dist/release/stats.json dist/release"
  },
  "keywords": [
    "webpack",
    "javascript"
  ],
  "author": "Chernetsky Ilya <ilya@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "file-loader": "^6.0.0",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webp": "^5.1.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jquery": "^3.5.0",
    "jsdom": "^16.2.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "nodemon": "^2.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
